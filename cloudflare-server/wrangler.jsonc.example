{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "cloudflare-server",
	"main": "src/index.ts",
	"compatibility_date": "2025-06-17",
	"assets": {
		"binding": "ASSETS",
		"directory": "./public"
	},
	"observability": {
		"enabled": true
	},
	"compatibility_flags": [
		"nodejs_compat"
	],
	"d1_databases": [
		{
			"binding": "DB NAME",
			"database_name": "DB NAME",
			"database_id": "YOUR ID"
		}
	]
}